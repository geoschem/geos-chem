#include atoms.kpp     { Periodic table information               }

#DEFVAR
CH4              = IGNORE;  { Active methane species          }
CH4_0001         = IGNORE;  { Active methane Jacobian tracer) }
CH4_0002         = IGNORE;  { Active methane Jacobian tracer) }
CH4_0003         = IGNORE;  { Active methane Jacobian tracer) }
CH4_0004         = IGNORE;  { Active methane Jacobian tracer) }
CH4_0005         = IGNORE;  { Active methane Jacobian tracer) }
CO               = IGNORE;  { Active carbon monoxide species  }
CO2              = IGNORE;  { Active carbon dioxide species   }
PCOfromCH4       = IGNORE;  { Tracks P(CO) from CH4      for diagnostics }
PCOfromCH4_0001  = IGNORE;  { Tracks P(CO) from CH4_0001 for diagnostics }
PCOfromCH4_0002  = IGNORE;  { Tracks P(CO) from CH4_0002 for diagnostics }
PCOfromCH4_0003  = IGNORE;  { Tracks P(CO) from CH4_0003 for diagnostics }
PCOfromCH4_0004  = IGNORE;  { Tracks P(CO) from CH4_0004 for diagnostics }
PCOfromCH4_0005  = IGNORE;  { Tracks P(CO) from CH4_0005 for diagnostics }
PCOfromNMVOC     = IGNORE;  { Tracks P(CO) from NMVOC for diagnostics }
LCH4byOH         = IGNORE;  { Dummy spc to track loss of CH4      by OH }
LCH4_0001byOH    = IGNORE;  { Dummy spc to track loss of CH4_0001 by OH }
LCH4_0002byOH    = IGNORE;  { Dummy spc to track loss of CH4_0002 by OH }
LCH4_0003byOH    = IGNORE;  { Dummy spc to track loss of CH4_0003 by OH }
LCH4_0004byOH    = IGNORE;  { Dummy spc to track loss of CH4_0004 by OH }
LCH4_0005byOH    = IGNORE;  { Dummy spc to track loss of CH4_0005 by OH }
LCH4byCl         = IGNORE;  { Dummy spc to track loss of CH4      by Cl }
LCH4_0001byCl    = IGNORE;  { Dummy spc to track loss of CH4_0001 by Cl }
LCH4_0002byCl    = IGNORE;  { Dummy spc to track loss of CH4_0002 by Cl }
LCH4_0003byCl    = IGNORE;  { Dummy spc to track loss of CH4_0003 by Cl }
LCH4_0004byCl    = IGNORE;  { Dummy spc to track loss of CH4_0004 by Cl }
LCH4_0005byCl    = IGNORE;  { Dummy spc to track loss of CH4_0005 by Cl }
LCH4inStrat      = IGNORE;  { Dummy spc to track loss of CH4      in strat }
LCH4_0001inStrat = IGNORE;  { Dummy spc to track loss of CH4_0001 in strat }
LCH4_0002inStrat = IGNORE;  { Dummy spc to track loss of CH4_0002 in strat }
LCH4_0003inStrat = IGNORE;  { Dummy spc to track loss of CH4_0003 in strat }
LCH4_0004inStrat = IGNORE;  { Dummy spc to track loss of CH4_0004 in strat }
LCH4_0005inStrat = IGNORE;  { Dummy spc to track loss of CH4_0005 in strat }
LCObyOH          = IGNORE;  { Dummy spc to track loss of CO       by OH    }
LCOinStrat       = IGNORE;  { Dummy spc to track loss of CO       in strat }

#DEFFIX
FixedOH       = IGNORE;  { Externally-supplied OH concentration     }
FixedCl       = IGNORE;  { Externally-supplied Cl concentration     }
DummyCH4trop  = IGNORE;  { Dummy placeholder for CH4   reactant     }
DummyCH4_0001trop  = IGNORE;  { Dummy placeholder for CH4   reactant     }
DummyCH4_0002trop  = IGNORE;  { Dummy placeholder for CH4   reactant     }
DummyCH4_0003trop  = IGNORE;  { Dummy placeholder for CH4   reactant     }
DummyCH4_0004trop  = IGNORE;  { Dummy placeholder for CH4   reactant     }
DummyCH4_0005trop  = IGNORE;  { Dummy placeholder for CH4   reactant     }
DummyCH4strat = IGNORE;  { Dummy placeholder for CH4   reactant     }
DummyCH4_0001strat = IGNORE;  { Dummy placeholder for CH4   reactant     }
DummyCH4_0002strat = IGNORE;  { Dummy placeholder for CH4   reactant     }
DummyCH4_0003strat = IGNORE;  { Dummy placeholder for CH4   reactant     }
DummyCH4_0004strat = IGNORE;  { Dummy placeholder for CH4   reactant     }
DummyCH4_0005strat = IGNORE;  { Dummy placeholder for CH4   reactant     }
DummyNMVOC    = IGNORE;  { Dummy placeholder for NMVOC reactant     }

#EQUATIONS
//
// %%%%%%%%%%%%%%%%%%%%%%%%
// %%%%%  TROP CHEM   %%%%%
// %%%%%%%%%%%%%%%%%%%%%%%%
//
// Rate [1/s] is from JPL 1997
CH4 + FixedOH = LCH4byOH             : 2.45d-12*EXP(-1775.0d0/TEMP)*TROP;
CH4_0001 + FixedOH = LCH4_0001byOH   : 2.45d-12*EXP(-1775.0d0/TEMP)*TROP;
CH4_0002 + FixedOH = LCH4_0002byOH   : 2.45d-12*EXP(-1775.0d0/TEMP)*TROP;
CH4_0003 + FixedOH = LCH4_0003byOH   : 2.45d-12*EXP(-1775.0d0/TEMP)*TROP;
CH4_0004 + FixedOH = LCH4_0004byOH   : 2.45d-12*EXP(-1775.0d0/TEMP)*TROP;
CH4_0005 + FixedOH = LCH4_0005byOH   : 2.45d-12*EXP(-1775.0d0/TEMP)*TROP;
//
// Rate [1/s] is from Kirschke et al, 2013 (Nature Geosci.)
CH4      + FixedCl = LCH4byCl        : 9.60d-12*EXP(-1360.0d0/TEMP)*TROP;
CH4_0001 + FixedCl = LCH4_0001byCl   : 9.60d-12*EXP(-1360.0d0/TEMP)*TROP;
CH4_0002 + FixedCl = LCH4_0002byCl   : 9.60d-12*EXP(-1360.0d0/TEMP)*TROP;
CH4_0003 + FixedCl = LCH4_0003byCl   : 9.60d-12*EXP(-1360.0d0/TEMP)*TROP;
CH4_0004 + FixedCl = LCH4_0004byCl   : 9.60d-12*EXP(-1360.0d0/TEMP)*TROP;
CH4_0005 + FixedCl = LCH4_0005byCl   : 9.60d-12*EXP(-1360.0d0/TEMP)*TROP;
//
// Rate k_Trop(1) [1/s] is set to HEMCO-supplied P(CO) from CH4
DummyCH4trop       = CO    + PCOfromCH4        : k_Trop(1);
DummyCH4_0001trop  = CO    + PCOfromCH4_0001   : k_Trop(1);
DummyCH4_0002trop  = CO    + PCOfromCH4_0002   : k_Trop(1);
DummyCH4_0003trop  = CO    + PCOfromCH4_0003   : k_Trop(1);
DummyCH4_0004trop  = CO    + PCOfromCH4_0004   : k_Trop(1);
DummyCH4_0005trop  = CO    + PCOfromCH4_0005   : k_Trop(1);
//
// Rate k_trop(2) [1/s] is the output of rate law function GC_OHCO
CO  + FixedOH = LCObyOH              : k_Trop(2);
//
// DummyNMVOC is a placeholder species set to 1 molec/cm3.
// This is a KPP "trick" in order to set the final CO concentration
// to k_Trop(3) [molec/cm3/s] * dt [s], where dt is the chemistry
// timestep.  k_Trop(3) contains externally-supplied P(CO) from NMVOC.
DummyNMVOC    = CO    + PCOfromNMVOC : k_Trop(3);
//
// %%%%%%%%%%%%%%%%%%%%%%%%
// %%%%%  STRAT CHEM  %%%%%
// %%%%%%%%%%%%%%%%%%%%%%%%
//
// Rate k_Strat(1) [1/s] is set to HEMCO-supplied loss of CH4 by OH
CH4      = LCH4inStrat               : k_Strat(1);
CH4_0001 = LCH4_0001inStrat          : k_Strat(1);
CH4_0002 = LCH4_0002inStrat          : k_Strat(1);
CH4_0003 = LCH4_0003inStrat          : k_Strat(1);
CH4_0004 = LCH4_0004inStrat          : k_Strat(1);
CH4_0005 = LCH4_0005inStrat          : k_Strat(1);
//
// DummyCH4 is a placeholder species set to 1 molec/cm3.
// This is a KPP "trick" in order to set the final CO concentration
// to k_Strat(2) [molec/cm3/s] * dt [s], where dt is the chemistry
// timestep.  k_Strat(2) contains externally-supplied strat P(CO).
DummyCH4strat      = CO              : k_Strat(2);
DummyCH4_0001strat = CO              : k_Strat(2);
DummyCH4_0002strat = CO              : k_Strat(2);
DummyCH4_0003strat = CO              : k_Strat(2);
DummyCH4_0004strat = CO              : k_Strat(2);
DummyCH4_0005strat = CO              : k_Strat(2);
//
// Rate k_Strat(3) [1/s] is set to HEMCO-supplied stratospheric loss of CO
CO            = LCOinStrat           : k_Strat(3);