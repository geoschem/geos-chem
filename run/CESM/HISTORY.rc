  Budget.frequency:    {FREQUENCY}
  Budget.duration:     {DURATION}
  Budget.mode:         'time-averaged'
  Budget.fields:       'BudgetChemistryFull_O3            ',
                       'BudgetChemistryTrop_O3            ',
                       'BudgetChemistryPBL_O3             ',
                       'BudgetChemistryLevs1to35_O3       ',
