  LevelEdgeDiags.frequency:   ${RUNDIR_HIST_TIME_AVG_FREQ}
  LevelEdgeDiags.duration:    ${RUNDIR_HIST_TIME_AVG_DUR}
  LevelEdgeDiags.mode:        'time-averaged'
  LevelEdgeDiags.fields:      'Met_CMFMC                     ',
                              'Met_PEDGE                     ',
                              'Met_PEDGEDRY                  ',
                              'Met_PFICU                     ',
                              'Met_PFILSAN                   ',
                              'Met_PFLCU                     ',
                              'Met_PFLLSAN                   ',
