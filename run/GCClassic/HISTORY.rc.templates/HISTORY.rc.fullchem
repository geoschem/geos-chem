  ConcAfterChem.frequency:    ${RUNDIR_HIST_TIME_AVG_FREQ}
  ConcAfterChem.duration:     ${RUNDIR_HIST_TIME_AVG_DUR}
  ConcAfterChem.mode:         'time-averaged'
