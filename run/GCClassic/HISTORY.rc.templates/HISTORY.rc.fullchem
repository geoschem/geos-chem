                              'RadClrSkyLWSurf_SU     ',
                              'RadClrSkyLWSurf_NI     ',
                              'RadClrSkyLWSurf_AM     ',
                              'RadClrSkyLWSurf_BC     ',
                              'RadClrSkyLWSurf_OA     ',
                              'RadClrSkyLWSurf_SS     ',
                              'RadClrSkyLWSurf_DU     ',
                              'RadClrSkyLWSurf_PM     ',
